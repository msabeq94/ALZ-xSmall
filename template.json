{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "routeTables_ALZ_hub_routetable_name": {
            "defaultValue": "ALZ-hub-routetable",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/routeTables",
            "apiVersion": "2024-01-01",
            "name": "[parameters('routeTables_ALZ_hub_routetable_name')]",
            "location": "eastus",
            "properties": {
                "disableBgpRoutePropagation": false,
                "routes": [
                    {
                        "name": "udr-default-azfw",
                        "id": "[resourceId('Microsoft.Network/routeTables/routes', parameters('routeTables_ALZ_hub_routetable_name'), 'udr-default-azfw')]",
                        "properties": {
                            "addressPrefix": "0.0.0.0/0",
                            "nextHopType": "VirtualAppliance",
                            "nextHopIpAddress": "52.170.221.71",
                            "hasBgpOverride": false
                        },
                        "type": "Microsoft.Network/routeTables/routes"
                    }
                ]
            }
        },
        {
            "type": "Microsoft.Network/routeTables/routes",
            "apiVersion": "2024-01-01",
            "name": "[concat(parameters('routeTables_ALZ_hub_routetable_name'), '/udr-default-azfw')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/routeTables', parameters('routeTables_ALZ_hub_routetable_name'))]"
            ],
            "properties": {
                "addressPrefix": "0.0.0.0/0",
                "nextHopType": "VirtualAppliance",
                "nextHopIpAddress": "52.170.221.71",
                "hasBgpOverride": false
            }
        }
    ]
}